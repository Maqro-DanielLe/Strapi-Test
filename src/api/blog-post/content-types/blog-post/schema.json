{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5,
      "default": 1
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "imageLink": {
      "type": "string"
    },
    "imageDescription": {
      "type": "text",
      "required": true,
      "default": "Stock image of shoes"
    },
    "date": {
      "type": "datetime",
      "required": true
    }
  }
}
